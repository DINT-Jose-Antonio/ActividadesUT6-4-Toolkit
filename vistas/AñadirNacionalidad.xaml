<Window x:Class="Actividades_UT6__1____Comandos.AñadirNacionalidad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Actividades_UT6__1____Comandos"
        mc:Ignorable="d"
        Title="Añadir nacionalidad" Height="129" Width="301" ShowInTaskbar="false" ResizeMode="NoResize"
        Icon="/Actividades UT6 -1 -  Comandos;component/assets/añadir.ico">
    <StackPanel Margin="10">
        <TextBlock>Nacionalidad</TextBlock>
        <TextBox Margin="0,0,0,10" Text="{Binding Path=Nacionalidad}"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Click="AceptarButton_Click" x:Name="AceptarButton" Width="80" Margin="0,0,5,0" IsDefault="true">Aceptar</Button>
            <Button Width="80" IsCancel="true">Cancelar</Button>
        </StackPanel>
    </StackPanel>
</Window>
